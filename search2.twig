<style type="text/css">
table.gridtable {
	font-family: verdana,arial,sans-serif;
	font-size:11px;
	color:#333333;
	border-width: 1px;
	border-color: #666666;
	border-collapse: collapse;
}
table.gridtable th {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #666666;
	background-color: #dedede;
}
table.gridtable td {
	border-width: 1px;
	padding: 8px;
	border-style: solid;
	border-color: #666666;
	background-color: #ffffff;
}
</style>

<table class="gridtable">
	<tr>
	<th>CollID</th>
	<th>Institution Code</th>
	<th>Collection Code</th>
	<th>Collection Name</th>
	<th>Collection icon</th>
	<th>accession</th>
	<th>family</th>
	<th>sciname</th>
	<th>tid</th>
	<th>author</th>
	<th>collector</th>
	<th>country</th>
	<th>state</th>
	<th>county</th>
	<th>observer UID</th>
	<th>Individual Count</th>
	<th>Life Stage</th>
	<th>Sex</th>
	<th>Locality</th>
	<th>Coll Number</th>
	<th>Habitat</th>
	<th>Date</th>
	<th>Event Date</th>
	<th>Elevation</th>
	</tr>
{% for key, value in return %}
	<tr>
    {% for k,v in value %}
    		<td>{{v}}</td>
    {% endfor %}
	</tr>
{% endfor %}
</table>
